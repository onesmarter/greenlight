<% @api.each do |item| %>
<%= item %>
<% end %>
<% if @api[:isForStartCall] %>
<%= form_for @user, url: "/b/"+@api[:room]+"/start",:html=> {:id => 'cForm'}, method: :post do |f|  %>
<%= f.submit %>
<% concat @api[:script] %>
<% else %>
DATA_START<% 
